<script>

var getDownloadLinkbtn = $("#getDownloadLinkbtn");

$("#free_download_form").on('submit', function(e){

    e.preventDefault();


    getDownloadLinkbtn.val("Sending Link...").prop("disabled", true);


    var formData = {
        item_id : "36447",
        uemail: $("#uemail").val(),
        subscribed: $("#free_download_subscribed").is(":checked"),
        action: 'send_download_link_2016'
    };
    
    var ajax_url = '/wp-admin/admin-ajax.php';

    $.post(ajax_url, formData, function(response) {
        
        getDownloadLinkbtn.val("Send Link Again").prop("disabled", false);

        var data = $.parseJSON(response);
        if(data.status) {
            

            if (typeof ga !== 'undefined') {
                //ga('send', 'pageview', '/thank-you-for-downloading');

                ga('send', {
                  hitType: 'event',
                  eventCategory: 'Download',
                  eventAction: 'Attempt'
                });
            }
    
            $("#download_feedback").html('<div class="alert alert-'+data.status+'"><strong>&#x2714; </strong>'+data.msg+'</div>');

        } else {
            $("#download_feedback").html('<div class="alert alert-danger"><strong>Error! </strong>Please contact support.</div>');
        }
    });


    return false;


});


</script>












